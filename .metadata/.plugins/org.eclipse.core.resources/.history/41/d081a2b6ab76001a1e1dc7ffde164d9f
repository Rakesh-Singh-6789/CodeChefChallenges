package QualificationRound;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.io.PrintWriter;
import java.util.StringTokenizer;

public class WorkAssignment {

	public static void main(String[] args) throws IOException {
		// TODO Auto-generated method stub
		BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
		PrintWriter out=new PrintWriter(System.out);
		int t=Integer.parseInt(br.readLine());
		int caseN=1;
		
		while(t-->0){
	       
			StringTokenizer st;
			int CameronEndTime=0;
			int JamieEndTime=0;
			
			
			
			int n=Integer.parseInt(br.readLine());
			StringBuilder sNew=new StringBuilder();
			boolean bl=true;
			while(n-->0){
				st=new StringTokenizer(br.readLine());
				int workStartTime=Integer.parseInt(st.nextToken());
				int workEndTime=Integer.parseInt(st.nextToken());
				
				
				if(workStartTime>=CameronEndTime && bl){
					sNew.append("J");
					CameronEndTime=workEndTime;
				}else if(workStartTime>=JamieEndTime && bl){
					sNew.append("C");
					JamieEndTime=workEndTime;
				}else if (bl){
					sNew=new StringBuilder();
					sNew.append("IMPOSSIBLE");
					bl=false;
					
				}
				
				
				
				
			}
			
			
			
			
		  
		    out.println("Case #"+(caseN++)+": "+sNew);		    //caseN++;
			
		}
		out.flush();
		out.close();
		

	}

}
