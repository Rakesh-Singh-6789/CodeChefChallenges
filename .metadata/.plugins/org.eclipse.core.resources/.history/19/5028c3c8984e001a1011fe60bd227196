package Assignment1;


import java.io.*;
import java.util.*;



public class CheckFirst {


	public static int totalSum=0;
	public static int count=0;
	public static void main(String[] args) throws IOException {
		// TODO Auto-generated method stub

		BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
		int cases=Integer.parseInt(br.readLine());
		StringTokenizer st;


		while(cases-->0){
			
			int request=Integer.parseInt(br.readLine());
			int arr[][]=new int[4][4];
			//Integer singleArr[]=new Integer[16];
			while(request-->0){
				st= new StringTokenizer(br.readLine());
				String m=st.nextToken();
				String t=st.nextToken();

				int row,col;

				if(m.equals("A")){
					row=0;
				}else if(m.equals("B")){
					row=1;
				}else if(m.equals("C")){
					row=2;
				}else{
					row=3;
				}

				if(t.equals("12")){
					col=0;
				}else if(t.equals("3")){
					col=1;
				}else if(t.equals("6")){
					col=2;
				}else{
					col=3;
				}
				//System.out.print(" "+m+" "+t);
				arr[row][col]+=1;



			}
			
			/*int vacantDay=0;
			for(int j=0;j<4;j++){
				boolean isEmpty=false;
				for(int i=0;i<4;i++){
					if(arr[i][j]==0){
						isEmpty=true;
					}else{
						isEmpty=false;
						break;
					}
				}
				if(isEmpty){
					vacantDay++;
				}
			}*/
			
           
			int num1,num2,num3,num4 = 0;
			long maxSum=Integer.MIN_VALUE,currentSum=0;
			for(int i=0;i<4;i++){
			  	
				HashSet<Integer> colSet=new HashSet<>(); 
				ArrayList<Integer> list;
				
				num1=arr[0][i];
				colSet.add(i);
				
				
				for(int j=0;j<4;j++){
					
					if(colSet.contains(j)) 
						continue;
					
					num2=arr[1][j];					
					colSet.add(j);
					
					
					for(int k=0;k<4;k++){
						
						if(colSet.contains(k))
                            continue;
						
						num3=arr[2][k];
						colSet.add(k);
						
						
						
						for(int l=0;l<4;l++){
							
							
							if(colSet.contains(l))
								continue;
							
							num4=arr[3][l];
							
							
							
						}
						
						/*for(int a: list){
							System.out.print(a+" ");
						}*/
						// System.out.println(num1+" "+num2+" "+ num3+" "+num4);
						//System.out.println();
						list=new ArrayList<>();
						list.add(num1);
						list.add(num2);
						list.add(num3);
						list.add(num4);
						Collections.sort(list,Collections.reverseOrder());
						currentSum=list.get(0)*100 + list.get(1)*75 +list.get(2)*50 +list.get(3)*25;
						
						int countZeroes=0;
						for(int a: list){
							if(a==0){
								countZeroes++;
						        }
						}
						currentSum=currentSum-(countZeroes*100);
						if(currentSum > maxSum){
							maxSum=currentSum;
							System.out.print("list"+" ");
							for(int a:list){
								System.out.print(a+" ");
							}
							System.out.println();
							System.out.println(num1+" "+num2+" "+ num3+" "+num4+" "+maxSum);

						}
						
						colSet.remove(k);
						
					}
					colSet.remove(j);
					
					
					
				}
				
				
			}
			totalSum+=maxSum;
			
			System.out.println(maxSum);
         
		}
		System.out.println(totalSum);

	}
}
